.card-block {background: #FFF;margin: 0px;padding: 0px 11px;font-size: 13px;height:100%;min-height:0px;max-height: 100px;overflow-y: auto;}
.card-block ul {margin:0;padding:13px 0;list-style-type:none;}
.card-block ul li {position:relative;margin: 0 0 4px 0;}
.card-block ul li i {text-align: right;position: absolute;right: 0px;top: 5px;color: rgb(47, 207, 6);font-size: 10px;cursor:pointer;}
.card-block ul li:hover {background:#f0eeee;}
.rbccard h5 i {position: absolute;right: 9px;top: 8px;color: #FFF;cursor: pointer;z-index: 1;font-size: 15px;transition: all 0.3s ease-in-out 0s; }
.rbccard h5 i:hover {transform: rotate(360deg);transition: all 0.3s ease-in-out 0s;}

.rbccard h5:hover .bubble {background:url('../images/bubble.png'); width:91px;height:60px;display:block!important;position: absolute;right: -93px;top: -39px;}

.rbccard h5:hover .bubble span {font-size: 12px;color: rgb(255, 255, 255);line-height: 9px;width: 84px;display: block;position: absolute;top: 15px;left: 7px;font-weight: bold;}
